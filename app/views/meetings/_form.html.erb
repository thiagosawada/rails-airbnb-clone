<%= simple_form_for(@meeting) do |m|  %>
  <%= m.error :base %>
  <%= m.input :name %>
  <%= m.input :city %>
  <%= m.input :location %>
  <%= m.input :duration %>
  <%= m.input :group_size %>
  <%= m.association :category %>
  <%= m.input :description %>
  <%= m.input :date %>
  <%= m.input :photo, as: :hidden %>
  <%= m.input :photo_cache, as: :hidden %>
  <%= m.submit%>
<% end %>



